package user;

import java.io.Serializable;

public class User implements Serializable{
	
	
	private String name;
	private String email;

	
	public User(final String name, final String email) {
        this.name = name;
        this.email = email;
    }
	
	public String getName() {
        return name;
    }
	
	public void setName(String name) {
        this.name = name;
    }
}
